{
   "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
         "jsdoc"
      ]
   },
   "source": {
      "include": [
         "src/app",
         "src/app/director-info",
         "src/app/favorite-movies",
         "src/app/genre-info",
         "src/app/movie-card",
         "src/app/movie-detail",
         "src/app/nav-bar",
         "src/app/search-filter",
         "src/app/user-login-form",
         "src/app/user-profile",
         "src/app/user-registration-form",
         "src/app/welcome-page"
      ],
      "includePattern": ".+\\.ts$",
      "excludePattern": "(node_modules/|docs)"
   },
   "plugins": [
      "node_modules/jsdoc-better-typescript"
   ],
   "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "default"
   }
}